{
  "name": "prisma-enterprise",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "preview": "vite preview",
    "format": "prettier --write .",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "check": "npm run sec:check && npm run lint && npm run typecheck && npm run build && npm run spell",
    "audit": "audit-ci --moderate",
    "lockfile:lint": "lockfile-lint -p package-lock.json -t npm --validate-https --allowed-hosts npm",
    "pkg:lint": "npmPkgJsonLint --config .npmPkgJsonLintrc.json .",
    "sec:check": "npm run lockfile:lint && npm run pkg:lint && npm run audit",
    "cz": "cz",
    "spell": "cspell --no-progress --no-summary --relative --cache .",
    "browserslist:update": "npx update-browserslist-db@latest --update-db",
    "size": "npm run build && size-limit",
    "sanity": "npm run sec:check && npm run lint && npm run typecheck && npm run build && npm run size && npm run spell"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vite-plugin-pwa": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cspell/cspell-bundled-dicts": "9.2.2",
    "@cspell/dict-css": "4.0.18",
    "@cspell/dict-en_us": "4.4.21",
    "@cspell/dict-git": "3.0.7",
    "@cspell/dict-html": "4.0.12",
    "@cspell/dict-node": "5.0.8",
    "@cspell/dict-npm": "5.2.19",
    "@cspell/dict-ru_ru": "2.3.2",
    "@cspell/dict-software-terms": "5.1.9",
    "@cspell/dict-typescript": "3.2.3",
    "@eslint/js": "^9.36.0",
    "@size-limit/file": "11.2.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "audit-ci": "7.1.0",
    "commitizen": "4.3.1",
    "cspell": "9.2.2",
    "cz-conventional-changelog": "3.3.0",
    "editorconfig": "^3.0.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.5",
    "lockfile-lint": "4.14.1",
    "npm-package-json-lint": "9.0.0",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "size-limit": "11.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.9"
  },
  "license": "MIT",
  "description": "Prisma Enterprise â€” React + TypeScript + Vite foundation",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnmbeloved002-lang/prisma-enterprise.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "120 kB"
    }
  ]
}
