{
  "name": "prisma-enterprise",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "preview": "vite preview",
    "format": "prettier --write .",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "check": "npm run sec:check && npm run lint && npm run typecheck && npm run build",
    "audit": "audit-ci --moderate",
    "lockfile:lint": "lockfile-lint -p package-lock.json -t npm --validate-https --allowed-hosts npm",
    "pkg:lint": "npmPkgJsonLint .",
    "sec:check": "npm run lockfile:lint && npm run pkg:lint && npm run audit"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "audit-ci": "7.1.0",
    "editorconfig": "^3.0.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.5",
    "lockfile-lint": "4.14.1",
    "npm-package-json-lint": "9.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": "prettier --write"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.9"
  },
  "license": "MIT",
  "description": "Prisma Enterprise â€” React + TypeScript + Vite foundation",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnmbeloved002-lang/prisma-enterprise.git"
  }
}
